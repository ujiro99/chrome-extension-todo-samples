{
  "manifest_version": 3,

  "name": "react-todo",
  "version": "0.2.0",

  "action": {
    "default_icon": "icon16.png",
    "default_popup": "index.html"
  },

  "background": {
    "service_worker": "eventPage.js"
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "permissions": [
    "storage"
  ]
}
